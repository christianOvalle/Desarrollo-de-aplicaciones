
@{
    ViewBag.Title = "MostrarCursos";
}

<h2>MostrarCursos</h2>
<div class="row">

    @foreach (ExamenASP.Models.Aula item in Model)
    {
    <div class="col-sm-3 col-lg-1 col-md-4">


        <h3> @item.NombreAula</h3>

        <h3> @item.TipoAula</h3>

        <h3> @item.CantidadDeEstudiante</h3>

        <h3> @item.taller.NombreDeTaller</h3>




    </div>

    }



</div>

